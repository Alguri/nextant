<?xml version="1.0" encoding="ISO-8859-1" ?>
<database>
	<name>*dbname*</name>
	<create>true</create>
	<overwrite>false</overwrite>
	<charset>utf8</charset>

	<table>
		<name>*dbprefix*nextant_index</name>
		<declaration>
			<field>
				<name>fileid</name>
				<type>integer</type>
				<default>0</default>
				<notnull>true</notnull>
				<length>4</length>
			</field>
			<field>
				<name>clef</name>
				<type>text</type>
				<notnull>true</notnull>
				<length>64</length>
			</field>
			<field>
				<name>creation</name>
				<type>timestamp</type>
				<default></default>
				<notnull>false</notnull>
			</field>
			<index>
				<name>nextant_clef</name>
				<unique>true</unique>
				<field>
					<name>fileid</name>
					<name>clef</name>
				</field>
			</index>
		</declaration>
	</table>
</database>